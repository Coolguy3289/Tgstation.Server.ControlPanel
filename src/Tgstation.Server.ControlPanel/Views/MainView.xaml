<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:local="clr-namespace:Tgstation.Server.ControlPanel.Views"
        xmlns:page="clr-namespace:Tgstation.Server.ControlPanel.Views.Pages"
        xmlns:vm="clr-namespace:Tgstation.Server.ControlPanel.ViewModels;assembly=Tgstation.Server.ControlPanel"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <Design.DataContext>
    <vm:MainWindowViewModel/>
  </Design.DataContext>

  <StackPanel Orientation="Horizontal">
    <local:ServerBrowser/>
    <StackPanel Orientation="Vertical">
      <Panel>
        <page:AddServer/>
        <page:Administration/>
        <page:UserManager/>
        <page:InstanceManager/>
        <page:InstanceUser/>
        <page:Repository/>
        <page:StaticFiles/>
        <page:Watchdog/>
        <page:ChatBot/>
        <page:DreamMaker/>
        <page:Byond/>
      </Panel>
      <local:Console/>
    </StackPanel>
  </StackPanel>
</UserControl>
